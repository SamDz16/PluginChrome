{
	"manifest_version": 2,
	"name": "Trigger Louise Algorithm",
	"description": "Give user possibility to choose a strategy",
	"version": "1.0",
	"author": "samy",
	"content_scripts": [
		{
			"matches": ["https://dbpedia.org/sparql"],
			"js": ["libs/vuejs_lib.js", "wasm_exec.js","content.js"]
		}
	],
	"permissions": [
        "https://cdn.jsdelivr.net/npm/vue@2/dist/vue.js"
    ],
	"content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.jsdelivr.net; object-src 'self'",
	"browser_action": {
		"default_icon": "icons/default_icon.png",
		"default_popup": "popup.html"
	},
	"icons": { 
		"16": "icons/default_icon.png",
		"48": "icons/default_icon.png",
	   "128": "icons/default_icon.png" 
	}
}
